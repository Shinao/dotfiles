# arrow up
"\e[A":history-search-backward
# arrow down
"\e[B":history-search-forward

# Because mouse are for weak
# Vimlike shortcuts
"\ej":history-search-forward
"\ek":history-search-backward
"\eh":backward-char
"\el":forward-char
"\em":"make re\C-m"
"\es":"\C-asudo \C-m"

# Insensitive case because we dont feel anything
set completion-ignore-case On

# Fuck twice key
set show-all-if-ambiguous on
